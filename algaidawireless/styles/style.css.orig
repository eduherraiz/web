

body {
    background: white;
    font-family: Verdana, sans-serif;
    font-size: 13px;
    color: black;
  }


div, img {
    margin: 0;
    padding: 0;
    border: none;
  }


#wrap {
    margin-left: auto;
    margin-right: auto;
    width: 800px;
    background: url('../images/borders.png') repeat-y;
  }


#header {
    marker-offset: none;
    height: 131px;
    background: url('../images/top2.jpg') no-repeat;
  }


#header h1 {
    display: none;
  }


#nav {
    background: #E5E4E5;
    text-align: center;
    vertical-align: middle;
    margin: 0 19px 0 20px;
    height: 2em;
  }


#nav a {
    color: black;
    line-height: 2em;
    padding: 0 5px 0 5px;
    text-decoration: none;
  }


#nav a:hover, #nav a.selected {
    color: #0071AC;
  }


#nav_gal {
    background: #EEEEEE;
    text-align: center;
    vertical-align: middle;
    margin: 0 19px 0 20px;
    height: 2em;
  }


#nav_gal a {
    color: black;
    line-height: 2em;
    padding: 0 5px 0 5px;
    text-decoration: none;
  }


#nav_gal a:hover, #nav_gal a.selected {
    color: #0071AC;
  }


#content {
    margin-top: 20px;
    margin-bottom: 20px;
    margin-left: 20px;
    margin-right: 20px;
    padding: 0 19px 0 20px;
  }
#content table{
   width:100%;
   
}


#content p:first-letter {
    margin-left: 0em;
  }


#content a {
    text-decoration: none;
    text-align: right;
  }


#content a:hover {
    text-decoration: underline;
  }


#content a.top {
    font-size: x-small;
    float: right;
  }


#content h1 {
    color : #0071AC;
    font-size : 24px;
    font-style : oblique;
    text-align : center;
    width:80%;
  }


#content h3 {
    color : #0071AC;
    font-size : 16px;
    font-style : oblique;
  }


#footer {
    background: url('../images/bottom.png') no-repeat;
    height: 127px;
    text-align: center;
    padding: 0px 24px 0 25px;
  }


#footer a {
    color: #0071AC;
  }


#footer a:hover {
    color: white;
  }


#footer p {
    padding-top: 20px;
    color: white;
  }


pre {
    margin: 0 1em 0 1em;
    padding: 5px;
    background: #EFEFEF;
    border: 1px #CCCCCC;
    border-style: solid none solid;
  }


dl {
    margin-left: 1em;
    margin-top: 0.2em;
    margin-bottom: 0.5em;
  }


dt {
    font-weight: bold;
    margin-bottom: 0.1em;
  }


dd {
    line-height: 1.5em;
    margin-left: 2em;
    margin-bottom: 0.1em;
  }


abbr, acronym {
    border-bottom: 1px dotted black;
    cursor: help;
    letter-spacing: normal;
  }


table {
    font-size : 100%;
    margin-left : 1em;
  }


#fecha {
    color : #0071AC;
    font-size : 12px;
    width:100%;
    font-weight : bold;
  }


#phps {
    color : #0071AC;
    font-size : 14px;
  }


#new {
    margin-bottom: 20px;
    margin-right: 20px;
    padding: 0 19px 0 20px;
  }


#new p:first-letter {
    margin-left: 0em;
  }


#new a {
    text-decoration: none;
  }


#new a:hover {
    text-decoration: underline;
  }


#new a.top {
    font-size: x-small;
    float: right;
  }


#texto {
    margin-top: 20px;
    margin-bottom: 20px;
    margin-left: 20px;
    margin-right: 20px;
    padding: 0 19px 0 20px;
  }
#texto p {
    font-size: 11pt;
  }


